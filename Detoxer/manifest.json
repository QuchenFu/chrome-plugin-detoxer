{
  "name": "Detoxicater",
  "manifest_version": 3,
  "version": "0.1",
  "description": "Turns the page red when you click the icon",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
      "default_icon": {
          "16": "icons/icon16.png",
	        "32": "icons/icon32.png",
  	      "48": "icons/icon48.png",
  	      "128": "icons/icon128.png"
       }
   }
}
